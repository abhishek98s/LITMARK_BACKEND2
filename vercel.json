{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "package.json",
                    "package-lock.json",
                    "src/**"
                ],
                "installCommand": "npm ci",
                "buildCommand": "npm run build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "build/index.js"
        }
    ],
    "dev": {
        "command": "vercel dev"
    },
    "outputDirectory": "build"
}